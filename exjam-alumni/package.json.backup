{
  "name": "exjam-alumni",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "node scripts/validate-env.js && next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit -p tsconfig.strict.json",
    "validate:all": "tsx scripts/validate-all.ts",
    "fix-lint": "tsx scripts/fix-lint.ts",
    "system-check": "tsx scripts/system-check.ts",
    "health-check": "tsx scripts/system-check.ts",
    "postinstall": "prisma generate",
    "create-admin": "node scripts/create-admin.js",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "validate-env": "tsx scripts/validate-env.ts",
    "check-env": "tsx scripts/validate-env.ts",
    "perf:test": "node scripts/performance-test.js",
    "perf:build": "npm run build && npm run perf:test",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html --chrome-flags='--headless'",
    "perf:bundle": "npm run build:analyze",
    "perf:full": "npm run build && npm run start & sleep 10 && npm run perf:test && kill %1",
    "storage:setup": "tsx scripts/migrate-storage-assets.ts",
    "storage:migrate": "tsx scripts/migrate-storage-assets.ts",
    "functions:deploy": "./scripts/deploy-functions.sh",
    "functions:test": "node scripts/test-edge-functions.js",
    "api:test": "node scripts/test-api-routes.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
